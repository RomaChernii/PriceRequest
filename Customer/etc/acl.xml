<?xml version="1.0"?>
<!--
/**
 * PriceRequest Customer acl
-->
<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:Acl/etc/acl.xsd">
    <acl>
        <resources>
            <resource id="Magento_Backend::admin">
                <resource id="Magento_Customer::customer">
                    <resource id="PriceRequest_Customer::request" title="Price Request" translate="title" sortOrder="10">
                        <resource id="PriceRequest_Customer::request_save" title="Save Request" translate="title" sortOrder="10" />
                        <resource id="PriceRequest_Customer::request_delete" title="Delete Request" translate="title" sortOrder="20" />
                    </resource>
                </resource>
            </resource>
        </resources>
    </acl>
</config>
